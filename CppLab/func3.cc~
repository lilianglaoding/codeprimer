#include <iostream>
#include <string>

using namespace std;

bool str_subrange(const string &str1, const string &str2)
{
  if(str1.size() == str2.size())
    return str1 == str2;
  string::size_type size = (str1.size() < str2.size())
                           ? str1.size(), str2.size();
}

int main()
{
  string s1 = "hello, world";
  string s2 = "hello";
  return 0;
}

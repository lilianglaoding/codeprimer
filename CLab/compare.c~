#include <stdio.h>
#include <string.h>

int compare(const char *str1, const char *str2)
{
	while (*str1 != '\0')
        {
		if (*str1 < *str2)
			return -1;
 		else if (*str1 > *str2)
 			return 1;
  		else 
		{
			++str1;
			++str2;
		//	if (*str1 == '\0' || *str2 == '\0')
		//		return 0;
 		}
			
	}
	return 0;
}

int main()
{
        char *str1 = "hello", *str2 = "hellp";
        printf("%d\n", compare(str1, str2));
   	return 0;
}

#include <stdio.h>
#include <stdlib.h>

#define NUM 5

typedef struct node {
    int data;
    struct node *next;
} LNode, *LinkList;

void MemFree(LinkList head)
{
    if (NULL == head)
        return ;
    LinkList p = NULL;
    while (head != NULL)
    {
        p = head;
        head = head->next;
        free(p);
    }
}

LinkList CreateList()
{
    LNode *head = NULL;
    head = (LNode *)malloc(sizeof(LNode));
    head->next = NULL;
    return head;
}

//preinsert
void InsertList(LinkList head)
{
    if (NULL == head)
        return;
    int i = 0;
    LNode *ptr = NULL;
    for (i = 0; i < NUM; i++)
    {
        ptr = (LNode *)malloc(sizeof(LNode));
        scanf("%d",&ptr->data);
        ptr->next = head->next;
        head->next = ptr;
    }
}

void OutputList(LinkList head)
{
    if (NULL == head)
        return;
    while (head->next != NULL)
    {
        printf("%-4d",head->next->data);
        head = head->next;
    }
    printf("\n");
}

int main()
{
    LinkList head = NULL;
    head = CreateList();
    InsertList(head);
    OutputList(head);
    MemFree(head);
    return 0;
}
